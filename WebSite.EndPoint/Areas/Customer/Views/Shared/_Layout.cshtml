                    <a asp-area="" asp-controller="Home" asp-action="index">
                        <img src="~/themesite/assets/images/logo.png" class="navbar-logo" alt="logo">
                    </a>
                </li>
                <li class="nav-item theme-text">
                    <a asp-area="Customer" asp-controller="Home" asp-action="Detail" class="nav-link">  دال مارکت</a>
                </li>
