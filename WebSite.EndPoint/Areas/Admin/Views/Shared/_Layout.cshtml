    <!--  BEGIN MAIN CONTAINER  -->
    <div class="main-container" id="container">

        <div class="overlay"></div>
        <div class="cs-overlay"></div>
        <div class="search-overlay"></div>

        <!--  BEGIN SIDEBAR  -->
        <div class="sidebar-wrapper sidebar-theme">

            <nav id="sidebar">
                <div class="profile-info">
                    <figure class="user-cover-image"></figure>
                    <div class="user-info">
                        <img src="~/themesite/assets/images/person.png" alt="avatar" />
                        <h6 class=""> کاربر  </h6>
                        <p class="">مدیر پروژه</p>
                    </div>
                </div>
               
                <div class="shadow-bottom"></div>
                <ul class="list-unstyled menu-categories" id="accordionExample">
                    <li class="menu active">
                        <a asp-controller ="ForVisitor" asp-action="Index" asp-area="Admin" aria-expanded="true" class="dropdown-toggle">
                            <div class="">
                                <span>آمار بازدید</span>
                            </div>
                        </a>
                    </li>
                    <li class="menu active">
                        <a asp-controller="Confirm" asp-action="InConfirmComments" asp-area="Admin" aria-expanded="true" class="dropdown-toggle">
                            <div class="">
                                <span>کامنت های تایید نشده</span>
                            </div>
                        </a>
                    </li>

                    <li class="menu active">
                        <a asp-controller="Confirm" asp-action="InConfirmProducts" asp-area="Admin" aria-expanded="true" class="dropdown-toggle">
                            <div class="">
                                 <span>کالاهای تایید نشده</span>
                            </div>
                        </a>
                    </li>

                    <li class="menu active">
                        <a asp-controller="Seller" asp-action="Index" asp-area="Admin" aria-expanded="true" class="dropdown-toggle">
                            <div class="">
                              <span>فروشنده ها</span>
                            </div>
                        </a>
                    </li>
                    <li class="menu active">
                        <a asp-controller="Seller" asp-action="Index" asp-area="Admin" aria-expanded="true" class="dropdown-toggle">
                            <div class="">
                             
                                <span>مشتری ها</span>
                            </div>
                        </a>
                    </li>
                    <li class="menu active">
                        <a asp-controller="Product" asp-action="Index" asp-area="Admin" aria-expanded="true" class="dropdown-toggle">
                            <div class="">
                               <span>همه ی کالاها</span>
                            </div>
                        </a>
                    </li>
                    <li class="menu active">
                        <a asp-controller="Booth" asp-action="Index" asp-area="Admin" aria-expanded="true" class="dropdown-toggle">
                            <div class="">
                               <span>همه ی غرفه ها</span>
                            </div>
                        </a>
                    </li>
                </ul>

            </nav>

        </div>
        <!--  END SIDEBAR  -->
        <!--  BEGIN CONTENT AREA  -->
        <div id="content" class="main-content">
            @RenderBody()
        </div>
        <div class="footer-wrapper">
            <div class="footer-section f-section-1">
