@model WebSite.EndPoint.Areas.Seller.Models.AuctionAddVm
@{
    ViewData["Title"] = "Create";
}
<div class="row m-5 p-5 form-group d-flex justify-content-center">
    <div class="col-md-4">
        <form asp-action="Create">
            <input asp-for="ProductId" type="hidden" class="form-control" />
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="StartDeadTime" class="control-label"></label>
                <input asp-for="StartDeadTime" class="form-control"/>
                <span asp-validation-for="StartDeadTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="EndDeadTime" class="control-label"></label>
                <input asp-for="EndDeadTime" class="form-control"/>
                <span asp-validation-for="EndDeadTime" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="ثبت مزایده جدید" class="btn btn-primary btn-block"/>
            </div>
        </form>
    </div>
</div>


